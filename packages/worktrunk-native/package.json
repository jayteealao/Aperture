{
  "name": "@aperture/worktrunk-native",
  "version": "0.1.0",
  "description": "Native Node.js addon for git worktree management using git2-rs",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "napi": {
    "name": "worktrunk-native",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest run",
    "universal": "napi universal",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "@types/node": "^20.11.19",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">= 18"
  },
  "license": "MIT",
  "keywords": [
    "git",
    "worktree",
    "napi",
    "rust",
    "native-addon"
  ]
}
